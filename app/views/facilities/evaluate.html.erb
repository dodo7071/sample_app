<% provide(:title, 'Create Evaluation') %>
<h1>Create Evaluation</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for Evaluation.new do |f| %>

	  <%= render 'shared/error_messages', object: f.object %>

      <%= f.label :activity_note %>
      <%= f.text_field :note %>

	  <%= f.label :activity_value %>
      <%= f.text_field :value %>

      <%= f.submit "Create Evaluation", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>
